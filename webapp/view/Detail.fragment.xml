<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:viz="sap.viz.ui5.controls" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds" xmlns:viz.data="sap.viz.ui5.data" height="100%"
	width="auto">
	
	<Dialog title="{/CUSTOMER_NAME}">
		<VBox class="sapUiSmallMargin" width="auto">
		<f:SimpleForm id="SimpleFormDisplay354wide" editable="false" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="3" labelSpanM="4"
			labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="4" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="1" columnsM="1"
			singleContainerFullSize="false">
			<f:content>
				<Label text="Age">
					<layoutData>
						<l:GridData span="XL1 L2 M2 S4"/>
					</layoutData>
				</Label>
				<Text id="nameText" text="{/CUSTOMER_AGE_ID}">
					<layoutData>
						<l:GridData span="XL1 L10 M10 S8"/>
					</layoutData>
				</Text>
				<Label text="City">
					<layoutData>
						<l:GridData span="XL1 L6 M2 S4"/>
					</layoutData>
				</Label>
				<Text text="{/CUSTOMER_CITY}">
					<layoutData>
						<l:GridData span="XL1 L6 M2 S8"/>
					</layoutData>
				</Text>
				<Label text="Email Address">
					<layoutData>
						<l:GridData span="XL1 L6 M2 S4"/>
					</layoutData>
				</Label>
				<Text text="{/CUSTOMER_EMAIL}">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S8"/>
					</layoutData>
				</Text>
				<Label text="Eduction">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S4"/>
					</layoutData>
				</Label>
				<Text id="countryText" text="{/CUSTOMER_EDUCATION}">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S8"/>
					</layoutData>
				</Text>
				<Label text="Job">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S4"/>
					</layoutData>
				</Label>
				<Text id="jobText" text="{/CUSTOMER_JOB}">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S8"/>
					</layoutData>
				</Text>
				<Label text="Gender">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S4"/>
					</layoutData>
				</Label>
				<Text id="genderText" text="{/CUSTOMER_GENDER_ID}">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S8"/>
					</layoutData>
				</Text>
				<Label text="Marital Status">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S4"/>
					</layoutData>
				</Label>
				<Text id="maritalStatusText" text="{/CUSTOMER_MARITAL_STATUS_ID}">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S8"/>
					</layoutData>
				</Text>
				<Label text="Timestamp">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S4"/>
					</layoutData>
				</Label>
				<Text id="timeStampText" text="{/CUSTOMER_TIMESTAMP}">
					<layoutData>
						<l:GridData span="XL1 L6 M6 S8"/>
					</layoutData>
				</Text>
				
			</f:content>
		</f:SimpleForm>
		</VBox>
		<beginButton>
			<Button text="Close" press="onCloseDialog"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>